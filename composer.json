{
  "name": "mosiyash/elasticsearch-odm",
  "type": "library",
  "keywords": [
    "elasticsearch",
    "elastic"
  ],
  "description": "Elasticsearch Object Document Mapper is build for PHP 5.4.0+. It is a wrapper for the official elasticsearch/elasticsearch-php client.",
  "homepage": "https://github.com/mosiyash/elasticsearch-odm",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Nikita Mosiyash",
      "email": "nikita.mosiyash@gmail.com"
    }
  ],
  "require": {
    "aura/di": "^2.0",
    "codeliner/array-reader": "^1.0",
    "doctrine/inflector": "^1.0",
    "elasticsearch/elasticsearch": "^2.0",
    "jan-swiecki/simple-annotations": "^0.3",
    "php": ">=5.4.0",
    "sebastian/comparator": "~1.2"
  },
  "require-dev": {
    "sebastian/diff": "*",
    "symfony/process": ">=2"
  },
  "autoload": {
    "psr-4": {
      "Mosiyash\\Elasticsearch\\": "src/"
    }
  },
  "extra": {
    "aura": {
      "type": "library"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mosiyash\\Elasticsearch\\": "tests/"
    }
  }
}